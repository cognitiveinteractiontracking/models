    <visual name='visual'>
      <geometry>
        <mesh filename="${computer_mouse_model}" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision name='collision'>
      <geometry>
        <mesh filename="${computer_mouse_model}" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>


  <!--FLASH DRIVE-->
  <xacro:property name="flash_drive" value="flash_drive"/>
  <xacro:property name="flash_drive_model" value="package://twb_gazebo/meshes/flash_drive/meshes/flash_drive.dae"/>
  <joint name="${name}_${flash_drive}_joint" type="fixed">
    <axis xyz="0 0 1"/>
    <origin xyz="-0.199638 0.709093 0.887372" rpy="0 0 0"/>
