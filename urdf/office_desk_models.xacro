    </visual>
    <collision name='collision'>
      <geometry>
        <mesh filename="${modern_office_chair_model}" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <gazebo>
    <static>true</static>
    <self_collide>0</self_collide>
    <kinematic>0</kinematic>
    <gravity>0</gravity>
  </gazebo>

</robot>