  <xacro:property name="hand_model" value="package://twb_gazebo/meshes/hand/hand.dae"/>
  <joint name="${name}_${hand}_joint" type="fixed">
    <axis xyz="0 0 1"/>
    <origin xyz="0.104994 0.170283 0.899521" rpy="0 0 -1.58103"/>
    <parent link="${name}"/>
    <child link="${hand}"/>
  </joint>

  <link name='${hand}'>
    <inertial>
